{
  "extends": ["airbnb"],
  "plugins": ["jsx-a11y"],
  "env": {
    "browser": true
  },
  "rules": {
    "semi": 0,
    "quotes": [
      2,
      "double",
      {
        "avoidEscape": true
      }
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}
